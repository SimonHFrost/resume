doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    link(href='screen.css', rel='stylesheet', media='screen')
    link(href='print.css', rel='stylesheet', media='print')
    link(href='main.css', rel='stylesheet', media='screen')
    title= locals.title
  body(class='container')
    h1(class='page-header')= locals.pageHeader
    div(class='external-links clearfix')
      div(class='span-8')= locals.websiteUrl
      div(class='span-8')= locals.githubUrl
      div(class='span-8 last')= locals.email
    != page.html
